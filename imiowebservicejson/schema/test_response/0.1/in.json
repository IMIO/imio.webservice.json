{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Json validation schema for input document",
    "description": "This schema is used to validate json data sent when making a request to an imio application.",
    "type": "object",
    "properties": {
        "request_id": {
            "description": "The uid of the request",
            "type": "string",
            "minLength": 32,
            "maxLength": 32
        }
    },
    "additionalProperties": false,
    "required": ["request_id"]
}
